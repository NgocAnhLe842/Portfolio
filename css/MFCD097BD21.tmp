@charset "utf-8";
/* CSS Document */
*{
	margin: 0;
	padding: 0;
}
.sreen{
	width: 100%;
	height: 100vh;
	background: rgb(237,164,108);
    background: linear-gradient(98deg, rgba(237,164,108,0.9836135137648809) 0%, rgba(133,241,238,0.9023810207676821) 52%, rgba(139,147,240,0.9668067910758054) 100%);
	position: relative;
}
.header{
	width: 1%;
	height:100vh;
	background-color:black;
}
.menu{
	width: 250px;
	height:100vh;
	background-color:black;
	opacity: 0.4;
   	align-items: center;
  	text-align: left;
}

ul {
  line-height:  50px;  
  text-transform:uppercase;
  letter-spacing:0.15em;
  font-size: 20px;
  font-weight: bolder;
  padding-left: 15px;
  position: relative''
}
a{
	color:#FFFFFF;
	padding-left: 2%;
    position: absolute
}
.round{
	width:400px;
	height:400px;
	border-radius:400px;
	background-color:white;
	position:fixed;
	z-index: 1000;
	top: 100px;
	left: 200px;
}


